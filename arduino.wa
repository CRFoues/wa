import "syscall/arduino"

var LED = arduino.GetPinLED()

fn init() {
	arduino.PinMode(LED, 1)
	arduino.Println("Wa/Arduino is running 😎")
}

fn main() {
	for {
		arduino.DigitalWrite(LED, arduino.HIGH)
		arduino.Delay(100)
		arduino.DigitalWrite(LED, arduino.LOW)
		arduino.Delay(900)
	}
}
