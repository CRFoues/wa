package main

type sp struct {
	a: *i32
}

type sc struct {
	b: i32
	sp
}

fn gen_scref() => *sc {
	var v sc
	var i i32
	i = 13
	v.a = &i
	v.b = 42
	return &v
}

fn gen_sc() => sc {
	var i i32 = 26
	var v sc
	v.a = &i
	v.b = 58
	return v
}

fn main() {
	i := gen_scref()
	println(*i.a)
	println(i.b)
	
	j := gen_sc()
	println(*j.a)
	println(j.b)
}
