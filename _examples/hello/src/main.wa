import "fmt"
import "3rdparty/pkg"
import "myapp/mymath"

fn main() {
	fmt.Println(40+2)
	pkg.Println(100+2)
	println(mymath.I8Max)

	println(sum(100))
	heart()
}

fn sum(n: int) => int {
	var v: int
	for i := 1; i <= n; i++ {
		v += i
	}
	return v
}

fn heart() {
	a := 0.0
	for y := 1.5; y > -1.5; y = y - 0.1 {
		for x := -1.5; x < 1.5; x = x + 0.05 {
			a = x*x + y*y - 1.0
			if a*a*a < x*x*y*y*y {
				print("@")
			} else {
				print(" ")
			}
		}
		println()
	}
}
