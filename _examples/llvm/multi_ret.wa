# Test the llvm backend.
# Test multiple return values.

type pair struct {
	a :f32
	b :f32
}

fn calc() => ([4]f32, pair, f32) {
	a := [4]f32{1.1, 2.11, 3.111, 4.1111}
	b := pair{-10.123, 0.123}
	return a, b, 2.4 + 1.3
}

fn main() {
	a, b, c := calc()
	println("{", a[0], ", ", a[1], ", ", a[2], ", ", a[3], "}")
	println("{", b.a, ", ", b.b, "}")
	println(c)
}
