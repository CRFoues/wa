# Test the llvm backend.
# Test read/write structs.

fn main() {
	test_struct(10)
	test_struct(11)
	test_struct(2)
	test_struct(13)
	test_struct(0)
}

type t0 struct {
	a :int64
	b :int32
	c :[4]f64
	d :uint32
}

fn test_struct(a: int) {
	s0 := t0{17, -668, [4]f64{2.71828, 0.717, 3.14159}, 234}
	switch a {
	case 10:
		println(s0.a)
	case 11:
		println(s0.b)
	case 13:
		println(s0.d)
	default:
		println(s0.c[a])
	}
}
