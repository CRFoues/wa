func ExampleHello {
	msg := "Hello, 世界"
	encoded := StdEncoding.EncodeToString([]byte(msg))

	// println(encoded)
	// SGVsbG8sIOS4lueVjA==

	decoded, err := StdEncoding.DecodeString(encoded)
	if err != nil {
		println("decode error:", err.Error())
		return
	}
	println(string(decoded))

	// Output:
	// Hello, 世界
}
