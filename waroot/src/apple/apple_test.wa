import "runtime"

// wa test apple
// wa test -target=mvp apple

func TestApple {
	apple := Apple()
	if runtime.WAOS == "mvp" {
		assert(apple == "apple-mvp", apple)
	} else {
		assert(apple == "apple", apple)
	}
}

func ExampleApple {
	if runtime.WAOS == "mvp" {
		println(Apple())
	} else {
		println(Apple()+"-mvp")
	}

	// Output:
	// apple-mvp
}

func ExamplePanic {
	panic("fuck panic")

	// Output(panic):
	// fuck panic
}
