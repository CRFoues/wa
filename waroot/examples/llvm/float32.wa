// Test the llvm backend.
// Test f32 operations.

type fff32 f32

func getPi() => fff32 {
	return 3.1415926535
}

func getE() => fff32 {
	return 2.71828
}

func minusf32(a: fff32) => fff32 {
	return 0 - a
}

func circle_area(r: fff32) => fff32 {
	return r * r * 3.1415926
}

func main {
	println(getPi())
	println(getE())
	println(minusf32(getPi()))
	println(minusf32(-1.3))
	println(circle_area(1.1))
	println(circle_area(getE()))
}
